{
  "name": "skeet-functions",
  "version": "0.9.1",
  "description": "Skeet Functions - SKeet Full-stack TypeScript Serverless Framework",
  "repository": "https://github.com/elsoul/skeet-cli.git",
  "author": "ELSOUL LABO B.V.",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "lint": "eslint --ext .ts,.js --fix .",
    "dev": "nodemon",
    "dev:login": "npx ts-node -r tsconfig-paths/register --transpile-only src/scripts/login.ts",
    "build": "npx ts-node build.ts",
    "serve": "firebase emulators:start",
    "shell": "yarn build && firebase functions:shell",
    "start": "node dist/index.js",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@google-cloud/pubsub": "3.4.1",
    "@skeet-framework/firestore": "^1.0.9",
    "crypto": "1.0.1",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "dotenv": "16.0.3",
    "firebase": "9.22.0",
    "firebase-admin": "11.9.0",
    "firebase-functions": "4.4.1",
    "node-fetch": "2.6.6",
    "typesaurus": "7"
  },
  "devDependencies": {
    "@types/node-fetch": "2.6.3",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "esbuild": "0.17.14",
    "eslint": "8.9.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-import": "2.25.4",
    "firebase-functions-test": "3.0.0",
    "nodemon": "2.0.20",
    "typescript": "5.0.4"
  }
}
